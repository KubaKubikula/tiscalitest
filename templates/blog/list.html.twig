{% extends 'base.html.twig' %}

{% block body %}
<div class="paginator">
        {% for i in 1..pages %}
        <a href="/{{ i }}">{{ i }}</a> |
        {% endfor %}
</div>

{% for item in articles %}
<div class="card" >
    <div class="image">
        <img src="{{ item["IMGURL"] }}" width="306" />
    </div>
    <div class="content">
        <h3><a href="/detail/{{ item["ITEM_ID"] }}">{{ item["PRODUCT"] }}</a></h3>
        <p>{{ item["PRICE_VAT"]|round }} Kƒç</p>
        <div class="footer">

        </div>
    </div>
</div>
{% endfor %}
<div class="clear"></div>
{% endblock %}




